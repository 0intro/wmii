#!9PREFIX/bin/rc
# activate shortcuts of the given mode

wmiir write /keys/grabkeyb 0
wmiir write /keys/lookup /mode/$1

plab_cmd=`{wmiir read /wm/event/pageupdate | sed -e 's/\\//'}
if (! ~ $#plab_cmd 0) eval $"plab_cmd

